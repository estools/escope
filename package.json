{
  "name": "escope",
  "description": "ECMAScript scope analyzer",
  "homepage": "http://github.com/estools/escope",
  "bugs": "http://github.com/estools/escope/issues",
  "main": "lib/index.js",
  "module": "lib/bundle.esm.js",
  "browser": "lib/bundle.min.js",
  "files": [
    "lib"
  ],
  "version": "4.0.0",
  "engines": {
    "node": ">=4.0"
  },
  "author": {
    "name": "Yusuke Suzuki",
    "email": "utatane.tea@gmail.com",
    "web": "http://github.com/Constellation"
  },
  "contributors": [],
  "keywords": [
    "scope",
    "analysis"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/estools/escope.git"
  },
  "dependencies": {
    "esrecurse": "^4.2.1",
    "estraverse": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "babel-plugin-transform-es2017-object-entries": "0.0.5",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "espree": "^7.1.0",
    "esprima": "^4.0.1",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rollup": "^2.18.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "license": "BSD-2-Clause",
  "nyc": {
    "branches": 14,
    "lines": 53,
    "functions": 57,
    "statements": 50,
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "test",
      "third_party"
    ]
  },
  "scripts": {
    "build-browser": "rollup -c",
    "build-watch": "babel src/**/*.js --out-dir lib --source-maps --watch",
    "build": "babel src/**/*.js --out-dir lib --source-maps",
    "unit-test": "nyc mocha --require @babel/register test/*.js --timeout 100000",
    "test": "npm run build && npm run unit-test && npm run lint",
    "lint": "eslint .",
    "jsdoc": "jsdoc src/*.js README.md"
  }
}
